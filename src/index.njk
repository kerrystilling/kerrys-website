                            <div data-hero-word-1 class="relative inline-block h:auto faux-frame">
                                <div class="relative inline">Web</div>
                                    <div class="absolute bottom:n6 sm:bottom:n8 left:n6 sm:left:n8 h:12 sm:h:16 w:12 sm:w:16 radius:full bg:black"></div>
                                    <div class="absolute bottom:n6 sm:bottom:n8 right:n6 sm:right:n8 h:12 sm:h:16 w:12 sm:w:16 radius:full bg:black"></div>
